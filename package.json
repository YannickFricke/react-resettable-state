{
  "name": "react-resettable-state",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "format:check": "prettier --check ./src ./Readme.md",
    "format:write": "prettier --write ./src ./Readme.md",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.38",
    "husky": ">=4",
    "jest": "^25.5.4",
    "lint-staged": ">=10",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  }
}
